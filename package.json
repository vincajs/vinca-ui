{
  "name": "vinca-ui",
  "version": "0.0.1",
  "description": "Vinca UI",
  "repository": "vincajs/vinca-ui",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "nuxt-module-build build",
    "prepack": "pnpm build",
    "dev": "nuxi dev docs",
    "play": "nuxi dev playground",
    "build:docs": "nuxi generate docs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit && nuxi typecheck docs",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare docs",
    "play:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare play",
    "release": "release-it",
    "test": "vitest"
  },
  "dependencies": {
    "@nuxt/kit": "^3.9.1",
    "@nuxtjs/color-mode": "^3.3.2",
    "@unocss/nuxt": "^0.58.3",
    "@unocss/reset": "^0.58.3",
    "@vueuse/core": "^10.7.1",
    "@vueuse/motion": "^2.0.0",
    "nuxt-icon": "^0.6.8",
    "radix-vue": "^1.3.0",
    "tailwind-variants": "^0.1.20",
    "unocss": "^0.58.3",
    "vue": "^3.4.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.2",
    "@unocss/eslint-plugin": "^0.58.3",
    "@iconify/json": "^2.2.167",
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^0.2.0",
    "@nuxt/module-builder": "^0.5.5",
    "@nuxt/schema": "^3.9.0",
    "@nuxt/test-utils": "^3.9.0",
    "@types/node": "^20.10.6",
    "changelogen": "^0.5.5",
    "eslint": "^8.56.0",
    "nuxt": "^3.9.1",
    "typescript": "^5.3.3",
    "unocss-preset-theme": "^0.12.0",
    "vitest": "^1.0.0"
  }
}
